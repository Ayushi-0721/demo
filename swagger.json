      name: search
      in: query
      required: false
      description: Search for the specified word
      schema:
        type: string
    moduleuuid:
      name: moduleuuid
      in: path
      description: Id of resource to be fetched
      required: true
      schema:
        type: string
        format: uuid
        example: cb437522-63b7-44f4-991d-736c25c12926
    id:
      name: id
      in: path
      description: Id of resource to be fetched
      required: true
      schema:
        type: string
        format: uuid
        example: 94cfbc0f-6152-4a79-b663-3f3c923ff25b
    select:
      name: select
      in: query
      description: Select required properties
      required: false
      schema:
        type: string
      example: id,name
    solutionsactionname:
      name: action-name
      in: path
      description: Action to be performed on solutions
      required: true
      schema:
        type: string
        enum:
          - associate
          - disassociate
